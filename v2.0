  { word:"streamline", tr:"dÃ¼zene sokmak", phonetic:"STRÄ°M-layn", type:"verb", example:"Streamline your daily processes.", status:"new", score:0 }
];

let words = JSON.parse(localStorage.getItem('wf-words') || 'null') || DEFAULT_WORDS;
let streak = parseInt(localStorage.getItem('wf-streak') || '0');
let daily = parseInt(localStorage.getItem('wf-daily') || '0');
let totalCorrect = parseInt(localStorage.getItem('wf-correct') || '0');
let totalAnswered = parseInt(localStorage.getItem('wf-answered') || '0');

// ===== Spaced Repetition KuyruÄŸu =====
// Normal akÄ±ÅŸ: words dizisini sÄ±rayla gez
// Zor/Bilmedim: kelimeyi kuyruÄŸun 3-5 sonrasÄ±na ekle
let cardQueue = []; // index listesi
let queuePos = 0;
let reviewMode = false; // Tekrar Yap modu mu?

let cardIndex = 0;
let isFlipped = false;
let quizType = 'mc';
let qCorrect = 0, qWrong = 0, qStreak = 0;
let quizActive = false;
let fillAnswer = '';
let wordFilter = 'all';
let learnedFilter = 'all';

function save() {
  localStorage.setItem('wf-words', JSON.stringify(words));
  localStorage.setItem('wf-streak', streak);
  localStorage.setItem('wf-daily', daily);
  localStorage.setItem('wf-correct', totalCorrect);
  localStorage.setItem('wf-answered', totalAnswered);
}

// ========== KUYRUK ==========
function buildQueue(pool) {
  // pool: words dizisindeki index'leri karÄ±ÅŸtÄ±r
  cardQueue = pool.map((_,i) => i);
  // karÄ±ÅŸtÄ±r
  for(let i = cardQueue.length-1; i>0; i--) {
    const j = Math.floor(Math.random()*(i+1));
    [cardQueue[i],cardQueue[j]] = [cardQueue[j],cardQueue[i]];
  }
  queuePos = 0;
}

function currentWord() {
  if(reviewMode) {
    return words[cardQueue[queuePos % cardQueue.length]];
  }
  return words[cardQueue[queuePos % cardQueue.length]];
}

function currentWordIndex() {
  return cardQueue[queuePos % cardQueue.length];
}

function insertDifficult(idx) {
  // 3-5 sonraya ekle
  const insertAt = Math.min(queuePos + 3 + Math.floor(Math.random()*3), cardQueue.length);
  cardQueue.splice(insertAt, 0, idx);
}

// ========== STATS ==========
function updateStats() {
  document.getElementById('statTotal').textContent = words.length;
  document.getElementById('statKnown').textContent = words.filter(w=>w.status==='known').length;
  const pct = totalAnswered > 0 ? Math.round((totalCorrect/totalAnswered)*100) : 0;
  document.getElementById('statScore').textContent = pct + '%';
  document.getElementById('streakCount').textContent = streak;
  const p = Math.min(daily, 10);
  document.getElementById('progressBar').style.width = (p*10) + '%';
  document.getElementById('progressText').textContent = p + ' / 10 kelime';
}

// ========== TABS ==========
const tabOrder = ['flashcard','quiz','learned','words','add'];
function switchTab(name) {
  document.querySelectorAll('.tab-content').forEach(t=>t.classList.remove('active'));
  document.querySelectorAll('.tab-btn').forEach(t=>t.classList.remove('active'));
  document.getElementById('tab-'+name).classList.add('active');
  const idx = tabOrder.indexOf(name);
  if(idx>=0) document.querySelectorAll('.tab-btn')[idx].classList.add('active');
  if(name==='words') renderWordList();
  if(name==='learned') renderLearnedList();
  if(name==='flashcard') loadCard();
  if(name==='quiz') startQuiz();
}

function switchTabDirect(name) { switchTab(name); }

// ========== FLASHCARD ==========
function initQueue() {
  const pool = words.map((_,i)=>i);
  buildQueue(pool);
}

function loadCard() {
  if(words.length === 0) return;
  if(cardQueue.length === 0) initQueue();
  const w = currentWord();
  if(!w) return;
  cardIndex = currentWordIndex();

  document.getElementById('fc-word').textContent = w.word;
  document.getElementById('fc-phonetic').textContent = w.phonetic || '';
  document.getElementById('fc-type').textContent = w.type || '';
  document.getElementById('fc-translation').textContent = w.tr;
  document.getElementById('fc-example').textContent = w.example || '';
  isFlipped = false;
  document.getElementById('flashcard').classList.remove('flipped');
  document.getElementById('card-actions').style.display = 'none';

  // Fav butonu gÃ¼ncelle
  const favBtn = document.getElementById('fav-btn');
  favBtn.textContent = w.fav ? 'â¤ï¸' : 'ğŸ¤';
  favBtn.classList.toggle('active', !!w.fav);

  // Rozetler
  const badges = document.getElementById('fc-badges');
  badges.innerHTML = '';
  if(reviewMode) {
    badges.innerHTML += `<span class="sr-badge">ğŸ” Tekrar Modu</span>`;
  }
  if(w.status === 'known') {
    badges.innerHTML += `<span class="learned-badge">âœ… Ã–ÄŸrenildi</span>`;
  }
  if(w.status === 'learning') {
    badges.innerHTML += `<span class="sr-badge">ğŸ”„ Tekrar Geliyor</span>`;
  }
}

function flipCard() {
  isFlipped = !isFlipped;
  document.getElementById('flashcard').classList.toggle('flipped', isFlipped);
  if(isFlipped) {
    document.getElementById('card-actions').style.display = 'flex';
    daily = Math.min(daily + 0.5, 10);
    save();
    updateStats();
  }
}

function nextCard() {
  queuePos++;
  loadCard();
}

function shuffleCards() {
  if(reviewMode) {
    const knownIdxs = words.map((w,i)=>w.status==='known'?i:-1).filter(i=>i>=0);
    buildQueue(knownIdxs);
  } else {
    initQueue();
  }
  loadCard();
  toast('ğŸ”€ KarÄ±ÅŸtÄ±rÄ±ldÄ±!');
}

function rateCard(rating) {
  const w = words[cardIndex];
  const idx = cardIndex;

  if(rating === 'knew') {
    w.score = (w.score||0) + 2;
    if(w.score >= 4) w.status = 'known';
    else w.status = 'learning';
    toast('âœ… Harika! +2 puan');
    streak++;
  } else if(rating === 'hard') {
    w.score = Math.max(0,(w.score||0));
    w.status = 'learning';
    // Spaced repetition: 3-5 sonra tekrar gÃ¶ster
    insertDifficult(idx);
    toast('ğŸ¤” YakÄ±nda tekrar gÃ¶sterilecek!');
  } else {
    // Bilmedim: daha yakÄ±n tekrar
    w.score = 0;
    w.status = 'learning';
    insertDifficult(idx);
    toast('ğŸ˜“ EndiÅŸelenme! 3 sonra tekrar gÃ¶sterilecek');
  }
  save();
  updateStats();
  setTimeout(()=>nextCard(), 300);
}

// ========== FAVORÄ° ==========
function toggleFav(e) {
  e.stopPropagation();
  const w = words[cardIndex];
  w.fav = !w.fav;
  const btn = document.getElementById('fav-btn');
  btn.textContent = w.fav ? 'â¤ï¸' : 'ğŸ¤';
  btn.classList.toggle('active', w.fav);
  save();
  toast(w.fav ? 'â¤ï¸ Favorilere eklendi!' : 'ğŸ¤ Favorilerden Ã§Ä±karÄ±ldÄ±');
}

// ========== TEKRAR YAP ==========
function startReview(type) {
  let pool;
  if(type === 'fav') {
    pool = words.map((w,i)=>w.fav?i:-1).filter(i=>i>=0);
  } else {
    pool = words.map((w,i)=>w.status==='known'?i:-1).filter(i=>i>=0);
  }
  if(pool.length === 0) {
    toast('âš ï¸ HenÃ¼z Ã¶ÄŸrenilmiÅŸ kelime yok!');
    return;
  }
  reviewMode = true;
  buildQueue(pool);
  switchTabDirect('flashcard');
  toast(`ğŸ” ${pool.length} kelimeyle tekrar baÅŸladÄ±!`);
}

function stopReview() {
  reviewMode = false;
  initQueue();
  loadCard();
  toast('âœ… Normal moda dÃ¶nÃ¼ldÃ¼');
}

// ========== QUIZ ==========
function startQuiz() {
  if(words.length < 4) { document.getElementById('quiz-mc').innerHTML = '<p style="color:var(--muted);text-align:center;padding:40px">Quiz iÃ§in en az 4 kelime gerekli.</p>'; return; }
  if(quizType === 'mc') loadMC();
  else loadFill();
}

function nextQuiz() {
  if(quizType==='mc') loadMC();
  else loadFill();
}

function loadMC() {
  document.getElementById('quiz-mc').style.display = 'block';
  document.getElementById('quiz-fill').style.display = 'none';
  const w = words[Math.floor(Math.random()*words.length)];
  document.getElementById('q-turkish').textContent = w.tr;
  document.getElementById('q-sent-text').textContent = w.example || 'Bu kelimeyi bir cÃ¼mlede kullan.';
  let opts = [w.word];
  let pool = words.filter(x=>x.word!==w.word);
  while(opts.length < 4 && pool.length > 0) {
    const r = pool.splice(Math.floor(Math.random()*pool.length),1)[0];
    opts.push(r.word);
  }
  opts = opts.sort(()=>Math.random()-0.5);
  const container = document.getElementById('q-options');
  container.innerHTML = '';
  opts.forEach(o => {
    const btn = document.createElement('button');
    btn.className = 'option-btn';
    btn.textContent = o;
    btn.onclick = () => checkMC(o, w.word, container);
    container.appendChild(btn);
  });
  quizActive = true;
}

function checkMC(chosen, correct, container) {
  if(!quizActive) return;
  quizActive = false;
  totalAnswered++;
  container.querySelectorAll('.option-btn').forEach(b => {
    b.disabled = true;
    if(b.textContent === correct) b.classList.add('correct');
    if(b.textContent === chosen && chosen !== correct) b.classList.add('wrong');
  });
  if(chosen === correct) {
    qCorrect++; qStreak++;
    totalCorrect++;
    toast('âœ… DoÄŸru! +' + qStreak + ' seri');
  } else {
    qWrong++; qStreak = 0;
    toast('âŒ YanlÄ±ÅŸ! DoÄŸrusu: ' + correct);
  }
  document.getElementById('quizCorrect').textContent = qCorrect;
  document.getElementById('quizWrong').textContent = qWrong;
  document.getElementById('quizStreak').textContent = qStreak;
  daily = Math.min(daily+1, 10);
  save(); updateStats();
}

function loadFill() {
  document.getElementById('quiz-mc').style.display = 'none';
  document.getElementById('quiz-fill').style.display = 'block';
  const w = words[Math.floor(Math.random()*words.length)];
  fillAnswer = w.word.toLowerCase();
  const sentence = w.example || `She is very ${w.word} today.`;
  const blanked = sentence.replace(new RegExp(w.word, 'gi'), '______');
  document.getElementById('q-fill-sentence').innerHTML = `"${blanked}"`;
  document.getElementById('q-fill-input').value = '';
  document.getElementById('q-fill-feedback').textContent = '';
  document.getElementById('q-fill-feedback').style.color = '';
  document.getElementById('q-fill-input').focus();
  quizActive = true;
}

function checkFill() {
  if(!quizActive) return;
  const input = document.getElementById('q-fill-input').value.trim().toLowerCase();
  const fb = document.getElementById('q-fill-feedback');
  totalAnswered++;
  if(input === fillAnswer) {
    qCorrect++; qStreak++; totalCorrect++;
    fb.textContent = 'âœ… DoÄŸru! Harika!';
    fb.style.color = '#5cf8d1';
    toast('âœ… MÃ¼kemmel!');
  } else {
    qWrong++; qStreak = 0;
    fb.textContent = `âŒ YanlÄ±ÅŸ. DoÄŸru cevap: "${fillAnswer}"`;
    fb.style.color = '#fc5c7d';
    toast('âŒ YanlÄ±ÅŸ cevap');
  }
  document.getElementById('quizCorrect').textContent = qCorrect;
  document.getElementById('quizWrong').textContent = qWrong;
  document.getElementById('quizStreak').textContent = qStreak;
  quizActive = false;
  daily = Math.min(daily+1, 10);
  save(); updateStats();
}

function toggleQuizType() {
  quizType = quizType === 'mc' ? 'fill' : 'mc';
  toast(quizType === 'mc' ? 'ğŸ¯ Ã‡oktan seÃ§meli mod' : 'âœï¸ Yazma modu');
  nextQuiz();
}

// ========== WORD LIST ==========
function setWordFilter(f, btn) {
  wordFilter = f;
  document.querySelectorAll('#tab-words .filter-btn').forEach(b=>b.classList.remove('active'));
  btn.classList.add('active');
  renderWordList();
}

function renderWordList() {
  const q = (document.getElementById('search-input')?.value || '').toLowerCase();
  const grid = document.getElementById('word-grid');
  let filtered = words.filter(w => {
    const matchText = w.word.toLowerCase().includes(q) || w.tr.toLowerCase().includes(q);
    if(!matchText) return false;
    if(wordFilter === 'all') return true;
    if(wordFilter === 'fav') return !!w.fav;
    return (w.status||'new') === wordFilter;
  });
  if(filtered.length === 0) {
    grid.innerHTML = `<div class="empty-state" style="grid-column:1/-1"><div class="empty-icon">ğŸ“­</div><p>Bu filtrede kelime bulunamadÄ±.</p></div>`;
    return;
  }
  grid.innerHTML = filtered.map(w => {
    const idx = words.indexOf(w);
    return `
    <div class="word-card" onclick="viewWord(${idx})">
      <div class="word-card-header">
        <div class="word-card-word">${w.word} <span class="word-card-fav">${w.fav?'â¤ï¸':''}</span></div>
        <div class="word-card-status status-${w.status||'new'}"></div>
      </div>
      <div class="word-card-type">${w.type||''} ${w.phonetic||''}</div>
      <div class="word-card-tr">${w.tr}</div>
      <div class="word-card-example">${w.example||''}</div>
    </div>`;
  }).join('');
}

// ========== LEARNED LIST ==========
function setLearnedFilter(f, btn) {
  learnedFilter = f;
  document.querySelectorAll('#tab-learned .filter-btn').forEach(b=>b.classList.remove('active'));
  btn.classList.add('active');
  renderLearnedList();
}

function renderLearnedList() {
  const grid = document.getElementById('learned-grid');
  let filtered = words.filter(w => {
    if(w.status !== 'known') return false;
    if(learnedFilter === 'fav') return !!w.fav;
    return true;
  });
  if(filtered.length === 0) {
    grid.innerHTML = `<div class="empty-state" style="grid-column:1/-1">
      <div class="empty-icon">ğŸŒ±</div>
      <p>HenÃ¼z Ã¶ÄŸrenilmiÅŸ kelime yok.<br>Flashcard'da "Bildim!" butonuna basÄ±nca buraya eklenir.</p>
    </div>`;
    return;
  }
  grid.innerHTML = filtered.map(w => {
    const idx = words.indexOf(w);
    return `
    <div class="word-card" onclick="viewWord(${idx})">
      <div class="word-card-header">
        <div class="word-card-word">${w.word} <span class="word-card-fav">${w.fav?'â¤ï¸':''}</span></div>
        <div style="font-size:10px;color:#5cf8d1;margin-top:4px">âœ… Ã–ÄŸrenildi</div>
      </div>
      <div class="word-card-type">${w.type||''} ${w.phonetic||''}</div>
      <div class="word-card-tr">${w.tr}</div>
      <div class="word-card-example">${w.example||''}</div>
    </div>`;
  }).join('');
}

function viewWord(i) {
  reviewMode = false;
  cardIndex = i;
  // KuyruÄŸun baÅŸÄ±na bu kelimeyi koy
  cardQueue.unshift(i);
  queuePos = 0;
  loadCard();
  switchTabDirect('flashcard');
}

// ========== ADD WORD ==========
function addWord() {
  const word = document.getElementById('new-word').value.trim();
  const tr = document.getElementById('new-tr').value.trim();
  if(!word || !tr) { toast('âš ï¸ Kelime ve anlamÄ± zorunlu!'); return; }
  words.push({
    word, tr,
    phonetic: document.getElementById('new-phonetic').value.trim(),
    type: document.getElementById('new-type').value,
    example: document.getElementById('new-example').value.trim(),
    status:'new', score:0, fav:false
  });
  ['new-word','new-tr','new-phonetic','new-example'].forEach(id=>document.getElementById(id).value='');
  save(); updateStats();
  toast('âœ… Kelime eklendi: ' + word);
}

function bulkAdd() {
  const lines = document.getElementById('bulk-input').value.trim().split('\n');
  let count = 0;
  lines.forEach(line => {
    const parts = line.split(',');
    if(parts.length >= 2) {
      words.push({ word:parts[0].trim(), tr:parts[1].trim(), example:parts[2]?.trim()||'', type:'noun', status:'new', score:0, fav:false });
      count++;
    }
  });
  document.getElementById('bulk-input').value = '';
  save(); updateStats();
  toast(`âœ… ${count} kelime eklendi!`);
}

// ========== TOAST ==========
let toastTimer;
function toast(msg) {
  const el = document.getElementById('toast');
  el.textContent = msg;
  el.classList.add('show');
  clearTimeout(toastTimer);
  toastTimer = setTimeout(()=>el.classList.remove('show'), 2500);
}

// ========== INIT ==========
initQueue();
loadCard();
updateStats();
</script>
